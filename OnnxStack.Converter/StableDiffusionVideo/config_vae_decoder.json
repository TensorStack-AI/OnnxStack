{
    "input_model": {
        "type": "PyTorchModel",
        "config": {
            "model_loader": "vae_decoder_load",
            "model_script": "models.py",
            "io_config": {
                "input_names": [ "latent_sample", "num_frames" ],
                "output_names": [ "sample" ]
            },
            "dummy_inputs_func": "vae_decoder_conversion_inputs"
        }
    },
    "passes": {
        "convert": {
            "type": "OnnxConversion",
            "config": {
                "target_opset": 20,
                "torch_dtype": "float32"
            }
        },
        "optimize": {
            "type": "OnnxFloatToFloat16",
            "config": {
                "keep_io_types": true
            }
        }
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "config": {
                "accelerators": [
                    {
                        "device": "gpu",
                        "execution_providers": [
                            "DmlExecutionProvider"
                        ]
                    }
                ]
            }
        }
    },
    "engine": {
        "target": "local_system",
        "log_severity_level": 1,
        "evaluate_input_model": false,
        "output_dir": ".olive-cache/models/vae_decoder"
    }
}
