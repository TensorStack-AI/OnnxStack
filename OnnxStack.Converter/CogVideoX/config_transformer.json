{
    "input_model": {
        "type": "PyTorchModel",
        "config": {
            "model_loader": "unet_load",
            "model_script": "models.py",
            "io_config": {
                "input_names": [ "hidden_states", "encoder_hidden_states", "timestep"],
                "output_names": [ "sample" ]
            },
            "dummy_inputs_func": "unet_conversion_inputs"
        }
    },
    "passes": {
        "convert": {
            "type": "OnnxConversion",
            "config": {
                "target_opset": 19,
                "device": "cuda",
                "save_as_external_data": true,
                "all_tensors_to_one_file": true
            }
        },
        "optimize": {
            "type": "OrtTransformersOptimization",
            "config": {
                "model_type": "unet",
                "opt_level": 0,
                "float16": true,
                "use_gpu": true,
                "keep_io_types": true,
                "save_as_external_data": true,
                "all_tensors_to_one_file": true,
                "num_heads": 30,
                "optimization_options": {
                    "enable_gelu": true,
                    "enable_layer_norm": false,
                    "enable_attention": true,
                    "enable_rotary_embeddings": false,
                    "use_multi_head_attention": false,
                    "disable_multi_head_attention_bias": false,
                    "enable_skip_layer_norm": false,
                    "enable_embed_layer_norm": false,
                    "enable_bias_skip_layer_norm": false,
                    "enable_bias_gelu": true,
                    "enable_gelu_approximation": true,
                    "enable_qordered_matmul": true,
                    "enable_shape_inference": true,
                    "enable_gemm_fast_gelu": false,
                    "group_norm_channels_last": true,
                    "enable_nhwc_conv": true,
                    "enable_group_norm": false,
                    "enable_skip_group_norm": false,
                    "enable_bias_splitgelu": true,
                    "enable_packed_qkv": true,
                    "enable_packed_kv": true,
                    "enable_bias_add": true
                }
            }
        }
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "config": {
                "accelerators": [
                    {
                        "device": "gpu",
                        "execution_providers": [
                            "CUDAExecutionProvider"
                        ]
                    }
                ]
            }
        }
    },
    "engine": {
        "target": "local_system",
        "log_severity_level": 1,
        "evaluate_input_model": false,
        "output_dir": ".olive-cache/models/transformer"
    }
}
